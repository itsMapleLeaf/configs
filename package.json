{
  "name": "@itsmapleleaf/configs",
  "version": "3.0.1",
  "description": "my cool and awesome configurator of various things",
  "authors": "MapleLeaf",
  "type": "module",
  "exports": {
    "./eslint": "./.eslintrc.base.cjs",
    "./prettier": "./.prettierrc.cjs",
    "./tsconfig": "./tsconfig.base.json",
    "./tsconfig.base": "./tsconfig.base.json"
  },
  "bin": {
    "setup-configs": "./dist/main.js"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "tsup-node src/main.ts --target=node16 --format=esm",
    "test-manual": "esmo test/run-test.ts",
    "test-lint": "eslint --ext js,ts,tsx test/lint",
    "release": "pnpm build && release-it",
    "lint-fix": "pnpm lint -- --fix"
  },
  "files": [
    "dist",
    "assets",
    ".eslintrc.base.cjs",
    ".prettierrc.cjs",
    "tsconfig.base.json"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "chalk": "^5.3.0",
    "cpy": "^10.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "execa": "^8.0.1",
    "lodash-es": "^4.17.21",
    "ora": "^7.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prompt-fns": "^0.1.0",
    "type-fest": "^4.3.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.5.9",
    "eslint": "^8.48.0",
    "esmo": "^0.17.0",
    "release-it": "^16.1.5",
    "tailwindcss": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "esbuild": "0.14.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
