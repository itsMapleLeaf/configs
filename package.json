{
  "name": "@itsmapleleaf/configs",
  "type": "module",
  "description": "my cool and awesome configs for linting and stuff",
  "authors": "MapleLeaf",
  "version": "1.0.1",
  "files": [
    "src",
    "assets",
    "eslint",
    "prettier",
    "tsconfig.app.json",
    "tsconfig.base.json",
    "tsconfig.lib.json"
  ],
  "dependencies": {
    "@types/eslint": "^8.2.0",
    "chalk": "5.0.0",
    "cpy": "8.1.2",
    "execa": "6.0.0",
    "ora": "6.0.1",
    "type-fest": "2.6.0"
  },
  "devDependencies": {
    "@types/node": "16.11.10",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.5.0",
    "release-it": "14.11.8",
    "typescript": "4.5.2"
  },
  "bin": {
    "setup-configs": "./src/setup.js"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "release": "pnpm typecheck && release-it"
  },
  "prettier": "./prettier",
  "eslintConfig": {
    "extends": [
      "./eslint"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
