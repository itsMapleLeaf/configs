{
  "name": "@itsmapleleaf/configs",
  "type": "module",
  "description": "my cool and awesome configurator of various things",
  "authors": "MapleLeaf",
  "version": "1.1.4",
  "files": [
    "dist",
    "assets",
    "eslint",
    "prettier",
    "tsconfig.base.json"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "tsup-node src/main.ts --target=node16 --format=esm",
    "test-manual": "esmo test/run-test.ts",
    "test-lint": "eslint --ext js,ts,tsx test/lint",
    "release": "pnpm build && release-it",
    "lint-fix": "pnpm lint -- --fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chalk": "^5.0.1",
    "cpy": "^9.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-canonical": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "execa": "^6.1.0",
    "lodash-es": "^4.17.21",
    "ora": "^6.1.1",
    "prettier": "^2.7.1",
    "prompt-fns": "^0.1.0",
    "type-fest": "^2.14.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/eslint": "^8.4.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "eslint": "^8.18.0",
    "esmo": "^0.16.3",
    "release-it": "^15.1.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "esbuild": "0.14.2"
  },
  "bin": {
    "setup-configs": "./dist/main.js"
  },
  "prettier": "./prettier",
  "publishConfig": {
    "access": "public"
  }
}
