{
  "name": "@itsmapleleaf/configs",
  "version": "3.0.0",
  "description": "my cool and awesome configurator of various things",
  "authors": "MapleLeaf",
  "type": "module",
  "exports": {
    "./eslint": "./.eslintrc.base.cjs",
    "./prettier": "./.prettierrc.cjs",
    "./tsconfig": "./tsconfig.base.json",
    "./tsconfig.base": "./tsconfig.base.json"
  },
  "bin": {
    "setup-configs": "./dist/main.js"
  },
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "tsup-node src/main.ts --target=node16 --format=esm",
    "test-manual": "esmo test/run-test.ts",
    "test-lint": "eslint --ext js,ts,tsx test/lint",
    "release": "pnpm build && release-it",
    "lint-fix": "pnpm lint -- --fix"
  },
  "files": [
    "dist",
    "assets",
    ".eslintrc.base.cjs",
    ".prettierrc.cjs",
    "tsconfig.base.json"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chalk": "^5.2.0",
    "cpy": "^9.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "execa": "^7.1.1",
    "lodash-es": "^4.17.21",
    "ora": "^6.3.0",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prompt-fns": "^0.1.0",
    "type-fest": "^3.8.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "eslint": "^8.38.0",
    "esmo": "^0.16.3",
    "release-it": "^15.10.1",
    "tailwindcss": "^3.3.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "esbuild": "0.14.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
