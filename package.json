{
  "name": "@itsmapleleaf/configs",
  "type": "module",
  "description": "my cool and awesome configurator of various things",
  "authors": "MapleLeaf",
  "version": "1.1.2",
  "files": [
    "dist",
    "assets",
    "eslint",
    "prettier",
    "tsconfig.base.json"
  ],
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "tsup-node src/main.ts --target=node16 --format=esm",
    "test-manual": "esmo test/run-test.ts",
    "test-lint": "eslint --ext js,ts,tsx test/lint",
    "release": "pnpm build && release-it",
    "lint-fix": "pnpm lint -- --fix"
  },
  "dependencies": {
    "@types/eslint": "^8.2.2",
    "@types/lodash-es": "^4.17.5",
    "chalk": "^5.0.0",
    "cpy": "^8.1.2",
    "execa": "^6.0.0",
    "lodash-es": "^4.17.21",
    "ora": "^6.0.1",
    "prompt-fns": "^0.1.0",
    "type-fest": "^2.9.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "esmo": "^0.14.0",
    "prettier": "^2.5.1",
    "release-it": "^14.12.3",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4",
    "vite": "^2.7.12"
  },
  "resolutions": {
    "esbuild": "0.14.2"
  },
  "bin": {
    "setup-configs": "./dist/main.js"
  },
  "prettier": "./prettier",
  "eslintConfig": {
    "extends": [
      "./eslint"
    ],
    "ignorePatterns": [
      "**/node_modules/**",
      "**/dist/**",
      "**/test/test-project/**"
    ],
    "root": true
  },
  "publishConfig": {
    "access": "public"
  }
}
